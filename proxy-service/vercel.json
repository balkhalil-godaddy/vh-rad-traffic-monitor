{
  "functions": {
    "api/proxy.js": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/proxy",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://balkhalil-godaddy.github.io"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, Cookie, X-Elastic-Cookie"
        }
      ]
    }
  ]
} 